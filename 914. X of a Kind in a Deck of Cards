/**
 * @param {number[]} deck
 * @return {boolean}
 */
var hasGroupsSizeX = function(deck) {
    const rec = new Map()
    deck.forEach(val => {
        rec.set(val, (rec.get(val) ?? 0) + 1)
    })
    const vals = [...rec.values()]
    const largestCommonDivider = (a, b) => {
        let num1 = a, num2 = b
        while (num2 !== 0) {
            const div = num1 % num2
            num1 = num2
            num2 = div
        }
        return num1
