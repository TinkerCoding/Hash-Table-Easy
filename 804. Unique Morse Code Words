/**
 * @param {string[]} words
 * @return {number}
 */
const uniqueMorseRepresentations = function (words) {
  const morseCode = [".-","-...","-.-.","-..",".","..-.","--.","....","..",".---","-.-",".-..","--","-.","---",".--.","--.-",".-.","...","-","..-","...-",".--","-..-","-.--","--.."]

  let inMorse = new Set();
  for (let word of words) {
    inMorse.add(
      word
        .split(``)
        .reduce((acc, cur) => (acc += morseCode[cur.charCodeAt(0) - 97]), ``)
    );
  }
  return inMorse.size;
};
